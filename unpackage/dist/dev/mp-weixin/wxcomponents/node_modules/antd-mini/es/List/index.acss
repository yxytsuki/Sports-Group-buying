.ant-list-radius{margin:0 24rpx}.ant-list-radius .ant-list-body{border-radius:24rpx}.ant-list-radius .ant-list-footer,.ant-list-radius .ant-list-header{padding:16rpx 0}.ant-list-footer,.ant-list-header{display:flex;align-items:center;padding:16rpx 24rpx;line-height:1.4;font-size:30rpx;color:var(--list-header-color,#999)}.ant-list-footer:empty,.ant-list-header:empty{display:none}.ant-list-body{position:relative;overflow:hidden}.ant-list-body>.ant-list-item:last-child .ant-list-item-line::after{display:none}