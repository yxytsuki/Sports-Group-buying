.ant-copilot-welcome {
  background-image: var(--welcome-background-color, #f4f6fd);
  border-radius: 24rpx;
  padding: 24rpx;
  box-sizing: border-box;
}
.ant-copilot-welcome-title {
  font-size: 40rpx;
  color: var(--welcome-text-color, rgba(0, 0, 0, 0.88));
  font-weight: 600;
  margin-bottom: 24rpx;
}
.ant-copilot-welcome-content {
  font-size: 32rpx;
  color: var(--welcome-text-color, rgba(0, 0, 0, 0.88));
  line-height: 48rpx;
}
