  }
}
@keyframes ant-popup-position-right-close {
  0% {
    right: 0;
  }
  100% {
    right: -100%;
  }
}
@keyframes ant-popup-position-center-close {
  0% {
    opacity: 1;
    transform: translate3d(-50%, -50%, 0) scale(1);
  }
  20% {
    opacity: 0.8;
    transform: translate3d(-50%, -50%, 0) scale(0.9);
  }
  100% {
