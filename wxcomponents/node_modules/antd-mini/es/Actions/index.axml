<import-sjs
  from="./check.sjs"
  name="sjs"
></import-sjs>
<view
  class="ant-copilot-actions {{className}}"
  style="{{style}}"
>
  <view
    class="ant-copilot-actions-item"
    hoverClass="ant-copilot-actions-item-hover"
    onTap="handleTapAction"
    data-action="{{item}}"
    a:for="{{items}}"
    a:for-index="index"
    a:for-item="item"
  >
    <ant-icon
      a:if="{{sjs.checkIcon(item.icon)}}"
      className="ant-copilot-actions-item-icon"
      type="{{item.icon}}"
    ></ant-icon>
    <image
      a:else
      class="ant-copilot-actions-item-icon"
      src="{{item.icon}}"
    ></image>
  </view>
</view>